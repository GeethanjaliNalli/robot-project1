{
  "test_case_id": "TC00002_Diary_NPB",
  "test_case_name": "New patient booking",
  "description": "Diary_ New patient booking",
  "priority": "Medium",
  "type": "Functional",
  "preconditions": [
    "User should be logged in on the diary screen."
  ],
  "postconditions": [
    "A booking should be made if required information was filled in and saved.",
    "If the user closes the screen or not all required fields are filled in, NO booking should be made."
  ],
  "test_data": [
    {
      "test_case": "TC_01",
      "patient_first_name": "John",
      "patient_last_name": "Doe",
      "dob": "1990-01-01",
      "gender": "Male",
      "contact_number": "9876543210",
      "email": "john.doe@example.com",
      "appointment_date": "2025-11-13",
      "appointment_time": "10:00",
      "diary": "Dr. Smith",
      "reason": "General Consultation",
      "expected_result": "Booking created and visible with Black 'A'-icon."
    },
    {
      "test_case": "TC_02_missing_fields",
      "patient_first_name": "Jane",
      "patient_last_name": "",
      "dob": "1992-05-10",
      "gender": "Female",
      "contact_number": "",
      "email": "jane.doe@example.com",
      "appointment_date": "2025-11-13",
      "appointment_time": "11:00",
      "diary": "Dr. Smith",
      "reason": "General Consultation",
      "expected_result": "Booking not created; error for missing required fields."
    },
    {
      "test_case": "TC_03_invalid_email",
      "patient_first_name": "Alice",
      "patient_last_name": "Brown",
      "dob": "1985-12-31",
      "gender": "Female",
      "contact_number": "1234567890",
      "email": "alice.brown@invalid",
      "appointment_date": "2025-11-13",
      "appointment_time": "12:00",
      "diary": "Dr. Smith",
      "reason": "Follow-up",
      "expected_result": "Booking not created; error for invalid email."
    },
    {
      "test_case": "TC_04_boundary_dob",
      "patient_first_name": "Baby",
      "patient_last_name": "Newborn",
      "dob": "2025-11-13",
      "gender": "Male",
      "contact_number": "5555555555",
      "email": "baby.newborn@example.com",
      "appointment_date": "2025-11-13",
      "appointment_time": "13:00",
      "diary": "Dr. Smith",
      "reason": "Newborn Checkup",
      "expected_result": "Booking created and visible with Black 'A'-icon."
    },
    {
      "test_case": "TC_05_past_appointment",
      "patient_first_name": "Elder",
      "patient_last_name": "Smith",
      "dob": "1940-06-15",
      "gender": "Male",
      "contact_number": "4444444444",
      "email": "elder.smith@example.com",
      "appointment_date": "2020-01-01",
      "appointment_time": "09:00",
      "diary": "Dr. Smith",
      "reason": "Annual Checkup",
      "expected_result": "Booking not created; error for past date."
    },
    {
      "test_case": "TC_06_close_without_save",
      "patient_first_name": "Test",
      "patient_last_name": "User",
      "dob": "1999-09-09",
      "gender": "Other",
      "contact_number": "3333333333",
      "email": "test.user@example.com",
      "appointment_date": "2025-11-13",
      "appointment_time": "14:00",
      "diary": "Dr. Smith",
      "reason": "Consultation",
      "expected_result": "Booking not created; user closed screen without saving."
    },
    {
      "test_case": "TC_07_duplicate_booking",
      "patient_first_name": "John",
      "patient_last_name": "Doe",
      "dob": "1990-01-01",
      "gender": "Male",
      "contact_number": "9876543210",
      "email": "john.doe@example.com",
      "appointment_date": "2025-11-13",
      "appointment_time": "10:00",
      "diary": "Dr. Smith",
      "reason": "General Consultation",
      "expected_result": "Booking not created; error for duplicate booking in same slot."
    }
  ],
  "locators": [
    "diary_screen",
    "new_booking_button",
    "patient_details_form",
    "save_button",
    "booking_confirmation_icon"
  ]
}
